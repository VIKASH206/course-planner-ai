{
  "framework": null,
  "installCommand": "npm ci",
  "buildCommand": "npm ci && npm run build",
  "outputDirectory": "dist/course-planner-frontend",
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*\\.(js|css|json|ico|png|jpg|jpeg|gif|svg|woff|woff2|ttf|eot|webmanifest))",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
