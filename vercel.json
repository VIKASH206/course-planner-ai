{
  "version": 2,
  "name": "course-planner-ai",
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/course-planner-frontend/browser"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "buildCommand": "cd frontend && npm install && npm run build -- --configuration production",
  "outputDirectory": "frontend/dist/course-planner-frontend/browser",
  "framework": "angular"
}
